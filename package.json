    "mailgen": "^2.0.10",
    "mocha": "^7.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-dropbox": "^1.0.1",
    "nexmo": "^2.6.0",
    "node-gyp": "^6.1.0",
    "node-pre-gyp": "^0.10.3",
