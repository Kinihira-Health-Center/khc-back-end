{
  "name": "khc",
  "version": "1.0.0",
  "description": "New design of imikino Web App",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc --reporter=text --reporter=lcov mocha --require @babel/register --require babel-polyfill ./server/tests/*.spec.js --timeout 2000 --exit",
    "server": "nodemon --exec babel-node ./server/index.js",
    "frontend": "npm run start --prefix front",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run frontend\"",
    "start": "npm run build && cross-env NODE_ENV=production babel-node dist/index.js",
    "build": "babel server -d dist",
    "createdb:dev": "npx sequelize-cli db:create",
    "createtables:dev": "npx sequelize-cli db:migrate",
    "seed:dev": "npx sequelize-cli db:seed:all",
    "droptables:dev": "npx sequelize-cli db:migrate:undo:all",
    "createdb:test": "cross-env NODE_ENV=test npx sequelize-cli db:create",
    "createtables:test": "cross-env NODE_ENV=test node_modules/.bin/sequelize db:migrate",
    "seed:test": "cross-env NODE_ENV=test node_modules/.bin/sequelize db:seed:all",
    "droptables:test": "cross-env NODE_ENV=test node_modules/.bin/sequelize db:migrate:undo:all",
    "createdb:pro": "cross-env NODE_ENV=production npx sequelize-cli db:create",
    "createtables:pro": "cross-env NODE_ENV=production npx sequelize-cli db:migrate",
    "seed:pro": "cross-env NODE_ENV=production npx sequelize-cli db:seed:all",
    "droptables:pro": "cross-env NODE_ENV=production npx sequelize-cli db:migrate:undo:all",
    "coveralls": "npm test && nyc report --reporter=text-lcov | coveralls",
    "kill": "Taskkill /IM node.exe /F",
    "lint": "eslint ./server"
  },
  "author": "Igor J.L. Ndiramiye",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/node": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.10.5",
    "@babel/runtime": "^7.11.1",
    "@hapi/joi": "^17.1.0",
    "@sendgrid/mail": "^6.5.3",
    "babel-core": "^6.26.3",
    "babel-install": "2.1.0",
    "babel-loader": "^8.0.6",
    "babel-node": "0.0.1-security",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "chai-passport-strategy": "^1.0.1",
    "cloudinary": "^1.19.0",
    "connect-multiparty": "^2.2.0",
    "cors": "^2.8.5",
    "coveralls": "^3.0.9",
    "cross-env": "^7.0.0",
    "dotenv": "^8.2.0",
    "dropbox": "^8.1.0",
    "env": "0.0.2",
    "express": "^4.17.1",
    "express-graphql": "^0.11.0",
    "fs": "0.0.1-security",
    "graphql": "^15.3.0",
    "http": "0.0.0",
